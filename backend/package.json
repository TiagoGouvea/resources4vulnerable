{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --config nodemon.json",
    "start-production": "pm2-runtime src/index.js",
    "build": "tsc --build",
    "test": "node node_modules/.bin/jest --config ./jest.config.js -i",
    "create": "sequelize db:create",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:create": "sequelize migration:create",
    "migrate:fresh": "ts-node-dev --transpileOnly --project tsconfig.json database/recreate.ts && npm run migrate",
    "migrate:fresh-production": "node database/recreate.js && npm run migrate",
    "seed": "ts-node-dev --transpileOnly --project tsconfig.json database/seeders/index.ts",
    "seed-production": "node database/seeders/index.js",
    "checks": "npm run check-prettier && npm run check-lint && npm run check-typescript",
    "check-prettier": "prettier-check \"src/**/*.{js,ts,tsx}\" && prettier-check \"database/**/*.{js,ts,tsx}\"",
    "check-lint": "eslint \"src/**/*.{js,ts,tsx}\" --max-warnings 0 && eslint \"database/**/*.{js,ts,tsx}\" --max-warnings 0",
    "check-typescript": "tsc --noEmit",
    "cronjob-metrics": "ts-node-dev --transpileOnly --project tsconfig.json src/utils/cronjob.ts",
    "setup": "npm run migrate && npm run seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "8.2.0",
    "eslint-plugin-jsdoc": "^22.1.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.0",
    "pm2": "^4.2.3",
    "require-all": "3.0.0",
    "rollbar": "2.15.0",
    "sequelize": "^5.21.5",
    "sequelize-cli": "5.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "3.5.30",
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.3",
    "@types/node": "^13.9.8",
    "@types/sequelize": "^4.28.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "nodemon": "^2.0.2",
    "prettier": "2.0.2",
    "prettier-check": "^2.0.0",
    "ts-jest": "^25.3.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  }
}
